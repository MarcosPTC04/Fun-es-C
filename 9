void calcularRaizes(double a, double b, double c) {
    double delta_val = delta(a, b, c);
    if (delta_val < 0) {
        printf("Nao existem raizes\n");
    } else if (delta_val == 0) {
        double raiz = -b / (2 * a);
        printf("Raiz unica: %lf\n", raiz);
    } else {
        double raiz1 = (-b + sqrt(delta_val)) / (2 * a);
        double raiz2 = (-b - sqrt(delta_val)) / (2 * a);
        printf("Raizes: %lf e %lf\n", raiz1, raiz2);
    }
}
